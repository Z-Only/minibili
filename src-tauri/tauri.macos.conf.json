{
    "$schema": "https://schema.tauri.app/config/2.0.0-rc",
    "identifier": "com.minibili.app",
    "bundle": {
        "targets": ["app", "dmg"],
        "macOS": {
            "minimumSystemVersion": "10.15",
            "exceptionDomain": "hdslb.com",
            "dmg": {
                "appPosition": {
                    "x": 180,
                    "y": 170
                },
                "applicationFolderPosition": {
                    "x": 480,
                    "y": 170
                },
                "windowSize": {
                    "height": 400,
                    "width": 660
                },
                "windowPosition": {
                    "x": 200,
                    "y": 180
                }
            }
        }
    }
}
